{
  "name": "@soybeanjs/vue-admin-layout",
  "version": "1.1.2",
  "private": false,
  "description": "a layout component of admin template, support vue2 and vue3",
  "author": "Soybean (honghuangdc@gmail.com)",
  "license": "MIT",
  "homepage": "https://github.com/honghuangdc/soybean-admin-layout#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/honghuangdc/soybean-admin-layout.git"
  },
  "bugs": {
    "url": "https://github.com/honghuangdc/soybean-admin-layout/issues"
  },
  "keywords": [
    "admin",
    "layout",
    "admin-layout",
    "vue2",
    "vue3"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run typecheck && vite build",
    "build:vercel": "cross-env VITE_IS_VERCEL=1 vite build",
    "typecheck": "vue-tsc --noEmit",
    "release": "standard-version",
    "pub": "npm publish --access=public",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@unocss/reset": "^0.45.6",
    "colord": "^2.9.3",
    "css-render": "0.15.10",
    "vue-demi": "^0.13.7"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.6",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@vitejs/plugin-vue": "^3.0.3",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.1",
    "eslint": "^8.22.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "typescript": "^4.7.4",
    "unocss": "^0.45.6",
    "vite": "3.0.7",
    "vite-plugin-dts": "^1.4.1",
    "vue": "^3.3.4",
    "vue-tsc": "^0.40.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
